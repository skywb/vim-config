{
  "suggest.noselect": true,
  "suggest.removeDuplicateItems": true,
  "coc.preferences.useQuickfixForLocations": true,
  "signature.enable":true,
  "snippets.excludePatterns": [
    "~/.vim/snippets/vim-snippet/"
  ],
  "snippets.ultisnips.directories": [
    "~/.vim/snippets/vim-snippet/"
  ],
  "workspace.rootPatterns": [".vim","compile_commands.json", ".git",".hg", ".projcet", "WORKSPACE"],
  "json.format.enable": true,
  "diagnostic.warningSign": "âš ",
  "diagnostic.errorSign": "x",
  "diagnostic.infoSign": "i",
  "diagnostic.hintSign": "H",
  "diagnostic.refreshOnInsertMode": false,
  "inlayHint.enable": false,
  "colors.enable": true,
  "translator.engines": [
    "google",
    "youdao"
  ],
  "coc.preferences.maxFileSize": "1000MB",
  "workspace.ignoredFolders": [
    "external/**",
    "bazel-bin/**",
    "bazel-*/**"
